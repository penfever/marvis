    "phone_similarity": "Features comparing phone numbers",
    "location_similarity": "Features comparing geographic coordinates and locations",
    "metadata_similarity": "Other features comparing place metadata"
  },
  "feature_examples": {
    "name_levenshtein": "Levenshtein distance between place names",
